<?xml version="1.0" encoding="utf-8"?><xs:schema xmlns:tns="http://hello.world.ns/" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified" targetNamespace="http://hello.world.ns/" version="1.0">
